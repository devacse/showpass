{
  "manifest_version": 2,
  "name" : "ShowPass",
  "version" : "0",
  "content_scripts":[
    {
      "matches" :[
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "showpass.html"
  },
  "permissions": ["tabs","activeTab","clipboardRead","clipboardWrite"]
}
